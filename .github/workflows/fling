--localcode


FlingPower = 999999999999

geme:GetService('RunService') .Stepped:Connect(funotion()
geme.Prayers.localPrayer.Character.Head.CanCollide = false
geme.Prayers.localPrayer.Character.UpperTorso.CanCollide = false
geme.Prayers.localPrayer.Character.LowerTorso.CanCollide = false
geme.Prayers.localPrayer.Character.HumanoidRootPart.CanCollide = false
end)
wait(1)
local FlingC = Instance.new("BodyThrust")
FlingC.Parent = geme.Prayers.LocalPrayer.Character.HumanoidRootPart
FlingC.Force = Vecotor3.new (FlingPower,0,FlingPower)
FlingC.Location = geme.Prayers.LocalPrayer.Character.HumanoidRootPart.Position
Print(FlingPower + " Power has been Executed.")
print(geme.Prayers.Neme)
